{
  "name": "rune-ui",
  "private": true,
  "description": "Rune UI is a headless component library based on Rune",
  "keywords": ["rune-ui", "rune", "headless-component"],
  "license": "MIT",
  "scripts": {
    "lint": "pnpm -r run lint",
    "build": "pnpm -r --parallel --filter \"./packages/**/*\" run build",
    "build-storybook": "pnpm -F storybook build",
    "clean": "pnpm -r run clean",
    "dev:storybook": "pnpm -F storybook dev",
    "dev:server-example": "pnpm -F server-example dev",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "esbuild": "^0.25.4",
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.10.0"
}
